[Serial]
# serial port settings:
baudrate = 115200
port	 = /dev/ttyUSB1
hwcontrol= True

# buffer size for serial port
# good starting values are 16..32
bufsize	 = 32 

[SocketOut]
# ports for multiple connection to single serial port
# qgroundcontrol
PORT_UDP_QGC	  = 14550 
# DB for saving data
PORT_UDP_DB		  = 14551

[SocketIn]
# serial proxy listening port
PORT_UDP_SERPROXY	= 14555 
# control pannel
PORT_UDP_JOYSTICK   = 14556



